        - const categoriesArr = [{href:'https://via.placeholder.com/150x200/0000FF/808080',caption:'Cup cakes'},{href:'https://via.placeholder.com/150x200/0000FF/808080',caption:'Cokies'},{href:'https://via.placeholder.com/150x200/0000FF/808080',caption:'Donut Cakes'},{href:'https://via.placeholder.com/150x200/0000FF/808080',caption:'Cup Cakes'},{href:'https://via.placeholder.com/150x200/0000FF/808080',caption:'Tower Cakes'},{href:'https://via.placeholder.com/150x200/0000FF/808080',caption:'Custard Cake'}]
        each item in categoriesArr 
          div(class='categories-row__item') 
            img(href=item.href)
            p #{item.caption}
    section(class='banner')
      div(class='container banner-row')
        - const bannerArr = [{caption:'Sale of 30 %',title:'Custard Cake',text:'Legetables and cheese'},{caption:'Special recipe',title:'Fruits Taste',text:'Made with meat, vegetables and cheese'},{caption:'Valentine’s day ',title:'Sale off 30%',text:'All Chocolate Products'}]
          each item in bannerArr 
              div(class='banner-row__item') 
                div(class='banner-row__item__caption') #{item.caption}
                div(class='banner-row__item__title')  #{item.title}
                div(class='banner-row__item__text') #{item.text}
                button(class='banner-row__item__button button--contained ') Order now
    section(class='specical-products')
      div(class='container specical-products-row')
        div(class='specical-products__preview')
          img(src='https://via.placeholder.com/200x420')

        div(class='specical-products__content')
          div(class=' specical-products__content-title') Specical Products
          div(class='specical-products__content-products')
            special-product(img_href='https://via.placeholder.com/150x200/0000FF/808080',sale_percent='42',is_new='true',stars='4',caption='Test 1',current_price='$20.0')
